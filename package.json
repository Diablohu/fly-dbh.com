    "dependencies": {
        "axios": "^0.21.1",
        "bind-event": "^1.1.2",
        "classnames": "^2.3.1",
        "koa-range": "^0.3.0",
        "koot": "^0.15.0-beta.22",
        "mime": "^2.5.2",
        "query-string": "^7.0.0"
    },
    "devDependencies": {
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
        "@babel/plugin-proposal-optional-chaining": "^7.14.5",
        "@types/classnames": "^2.3.1",
        "autoprefixer": "^10.2.6",
        "babel-eslint": "^10.1.0",
        "cssnano": "^5.0.6",
        "eslint": "^7.28.0",
        "eslint-config-koot": "^3.1.1",
        "husky": "^6.0.0",
        "lint-staged": "^11.0.0",
        "normalize.css": "^8.0.1",
        "prettier": "^2.3.1",
        "prettier-eslint": "^12.0.0",
        "pretty-ms": "^7.0.1",
