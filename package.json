        "simple-git": "^2.39.1",
        "string-replace-async": "^2.0.0",
        "svg-url-loader": "^7.1.1",
        "url-loader": "^4.1.0"
    },
    "bugs": {
        "url": "https://github.com/cmux/koot/issues"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,cjs,mjs,ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{json,md,css,less,sass,scss}": [
            "prettier --write"
        ]
    },
    "koot": {
        "version": "0.15.0-beta.22"
    }
}
